package se.lnu.mediatechnology.stickossdk.utils{	//------------------------------------------------------------------------------	//	// Import	//	//------------------------------------------------------------------------------		import se.lnu.mediatechnology.stickossdk.core.Session;		//------------------------------------------------------------------------------	//	// Public class	//	//------------------------------------------------------------------------------		/**	 *	 * DESC..	 *	 */	public class PauseTimerObject	{		//--------------------------------------------------------------------------		//		// Public properties		//		//--------------------------------------------------------------------------				public var pauseTicker:uint;		public var callback:Function;				//--------------------------------------------------------------------------		//		// Constructor method		//		//--------------------------------------------------------------------------				/**		 *		 * DESC..		 *		 */		public function PauseTimerObject() 		{					}				//--------------------------------------------------------------------------		//		// Public methods		//		//--------------------------------------------------------------------------				/**		 *		 * DESC..		 *		 */		public function update():void		{			pauseTicker -= (Session.application.timeElapsedBetweenFrames * Session.application.frameRate);		}	}}